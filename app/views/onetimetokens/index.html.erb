<p id="notice"><%= notice %></p>

<h1>Onetimetokens</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @onetimetokens.each do |onetimetoken| %>
      <tr>
        <td><%= onetimetoken.title %></td>
        <td><%= onetimetoken.content %></td>
        <td><%= link_to 'Show', onetimetoken %></td>
        <td><%= link_to 'Edit', edit_onetimetoken_path(onetimetoken) %></td>
        <td><%= link_to 'Destroy', onetimetoken, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Onetimetoken', new_onetimetoken_path %>
